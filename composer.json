{
    "name": "mrbrownnl/laravel-saml2",
    "description": "A Laravel package for Saml2 integration as a SP (service provider) for multiple IdPs, based on OneLogin toolkit which is much more lightweight than simplesamlphp.",
    "keywords": ["laravel","saml", "saml2", "onelogin"],
    "homepage": "https://github.com/mrbrownnl/laravel-saml2",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Jeroen de Bruijn",
            "email": "mrbrownnl@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-openssl": "*",
        "illuminate/support": ">=5.0.0",
        "onelogin/php-saml": "^4.1.0"
    },
    "require-dev": {
        "mockery/mockery": "1.5.*",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-0": {
            "MrBrownNL\\Saml2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrBrownNL\\Saml2\\Saml2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
